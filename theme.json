{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"settings": {
	  "appearanceTools": true,
	  "layout": {
		"contentSize": "1280px",
		"wideSize": "1680px"
	  },
	  "spacing": {
		"units": ["%", "px", "em", "rem", "vh", "vw"]
	  },
	  "typography": {
		"fontFamilies": [
		  {
			"fontFamily": "Raleway Italic",
			"name": "Raleway Italic",
			"slug": "raleway-italic",
			"fontFace": [
			  {
				"fontFamily": "Raleway",
				"fontStyle": "italic",
				"fontWeight": "400",
				"src": ["file:./assets/fonts/raleway-italic.ttf"]
			  }
			]
		  },
		  {
			"fontFamily": "Raleway Light",
			"name": "Raleway Light",
			"slug": "raleway-light",
			"fontFace": [
			  {
				"fontFamily": "Raleway",
				"fontStyle": "normal",
				"fontWeight": "300",
				"src": ["file:./assets/fonts/raleway-light.ttf"]
			  }
			]
		  },
		  {
			"fontFamily": "Raleway Medium",
			"name": "Raleway Medium",
			"slug": "raleway-medium",
			"fontFace": [
			  {
				"fontFamily": "Raleway",
				"fontStyle": "normal",
				"fontWeight": "500",
				"src": ["file:./assets/fonts/raleway-medium.ttf"]
			  }
			]
		  },
		  {
			"fontFamily": "Raleway Regular",
			"name": "Raleway Regular",
			"slug": "raleway-regular",
			"fontFace": [
			  {
				"fontFamily": "Raleway",
				"fontStyle": "normal",
				"fontWeight": "400",
				"src": ["file:./assets/fonts/raleway-regular.ttf"]
			  }
			]
		  },
		  {
			"fontFamily": "Antic Didone Regular",
			"name": "Antic Didone Regular",
			"slug": "antic-didone-regular",
			"fontFace": [
			  {
				"fontFamily": "Antic Didone",
				"fontStyle": "normal",
				"fontWeight": "400",
				"src": ["file:./assets/fonts/antic-didone-regular.ttf"]
			  }
			]
		  }
		],
		"fontSizes": [
		  {
			"name": "Small",
			"size": "0.875rem",
			"slug": "small"
		  },
		  {
			"name": "Medium",
			"size": "1rem",
			"slug": "medium"
		  },
		  {
			"name": "Large",
			"size": "1.5rem",
			"slug": "large"
		  },
		  {
			"name": "Extra Large",
			"size": "2rem",
			"slug": "extra-large"
		  }
		]
	  },
	  "color": {
		"defaultPalette": false,
		"palette": [
		  {
			"color": "#1b2323",
			"name": "Dark Grey",
			"slug": "dark-grey"
		  },
		  {
			"color": "#fbfefd",
			"name": "White",
			"slug": "white"
		  },
		  {
			"color": "#46c8b9",
			"name": "Teal",
			"slug": "teal"
		  },
		  {
			"color": "#8eb0dc",
			"name": "Sky Blue",
			"slug": "sky-blue"
		  },
		  {
			"color": "#6f8bd3",
			"name": "Bluey Blue",
			"slug": "bluey-blue"
		  }
		]
	  },
	  "useRootPaddingAwareAlignments": true
	},
	"styles": {
	  "color": {
		"text": "#2C2D2D",
		"background": "#fbfefd"
	  },
	  "typography": {
		"fontFamily": "var(--wp--preset--font-family--raleway-regular)",
		"fontSize": "var(--wp--preset--font-size--medium)"
	  },
	  "elements": {
		"heading": {
		  "typography": {
			"fontFamily": "var(--wp--preset--font-family--antic-didone-regular)",
			"color": "#2C2D2D"
		  }
		},
		"paragraph": {
		  "typography": {
			"fontFamily": "var(--wp--preset--font-family--raleway-regular)"
		  }
		},
		"link": {
		  "color": {
			"text": "#6f8bd3"
		  },
		  ":hover": {
			"color": {
			  "text": "#46c8b9"
			}
		  }
		},
		"caption": {
		  "color": {
			"text": "#2C2D2D"
		  }
		},
		"button": {
		  "color": {
			"text": "#fbfefd",
			"background": "#46c8b9"
		  }
		}
	  }
	},
	"templateParts": [
	  {
		"area": "header",
		"name": "header"
	  },
	  {
		"area": "footer",
		"name": "footer"
	  },
	  {
		"area": "footer_v2",
		"name": "footer v2"
	  }
	],
	"version": 3
  }
  